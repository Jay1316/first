<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="360" xmlns:fx="http://javafx.com/fxml/1" fx:controller="calculator.controller.CalculatorController">
    <children>
        <Button layoutX="126" layoutY="90" text="Answer" onAction="#handleCalculate" fx:id="calculate" />
        <Button layout="80" layoutY="90" text="Exit" onAction="#handleQuit" fx:id="quit" />
        <Label layoutX="126" layoutY="120" minHeight="16" minWidth="69" fx:id="label" />
    </children>
</AnchorPane>